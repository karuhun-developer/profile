---
interface Props {
	siteName?: string;
    description?: string;

}

const { siteName, description } = Astro.props;
---
<meta content="Karuhun Developer" name="author">
<meta property="og:site_name" content={siteName ?? 'Karuhun Developer'}>
<meta property="og:title" content={siteName ?? 'Karuhun Developer'}>
<meta property="og:type" content="website">
<meta property="og:url" content={Astro.url}>
<meta property="og:image" itemprop="image" content="/images/logo.png">
<meta property="og:description"
    content={description ?? "Karuhun Developer adalah software house berpengalaman yang menawarkan solusi pengembangan perangkat lunak handal untuk memenuhi kebutuhan bisnis Anda. Dengan tim ahli dan berpengalaman, kami siap membantu Anda menghadirkan teknologi terbaik."}>
<meta name="description"
    content={description ?? "Karuhun Developer adalah software house berpengalaman yang menawarkan solusi pengembangan perangkat lunak handal untuk memenuhi kebutuhan bisnis Anda. Dengan tim ahli dan berpengalaman, kami siap membantu Anda menghadirkan teknologi terbaik."}>
