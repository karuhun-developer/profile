                            alt="Aplikasi Koperasi Multicoop.id"
                            class="rounded w-100 img-fluid"
                            width={50}
                            height={50}
                        />
                        Aplikasi Koperasi Multicoop.id
                    </a>
                </div>
            </div>
        </div>

        <!-- clients -->
        <OurClient />
        <!-- /clients -->
    </section>
    <!-- /product -->
</Layout>
